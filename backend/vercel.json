{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/auth",
            "dest": "/api/auth.js"
        },
        {
            "src": "/api/v1/category",
            "dest": "/api/category.js"
        },
        {
            "src": "/api/v1/products",
            "dest": "/api/products.js"
        }
    ]
}